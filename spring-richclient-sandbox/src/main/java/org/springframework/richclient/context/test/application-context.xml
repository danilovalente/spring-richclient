<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="messageSource"
		class="org.springframework.context.support.ResourceBundleMessageSource">
		<property name="basenames">
			<list>
				<value>
					org.springframework.richclient.context.test.messages
				</value>
			</list>
		</property>
	</bean>

	<bean name="testBean" class="org.springframework.richclient.context.test.TestBean">
	
	</bean>
	
	<bean scope="window" name="testCommand" class="org.springframework.richclient.context.test.TestCommand"/>

</beans>